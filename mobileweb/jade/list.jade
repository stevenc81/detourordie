header.bar-title
    a.button-prev(ng-click="back2Map()") 返回
    h1.title 臨檢站列表

.content
    ul.list(ng-show="checkpoints.length > 0")
        li(ng-repeat="checkpoint in checkpoints", ng-click="pinOnMap(checkpoint)", ng-class="{new_item: checkpoint.age < 2, two_hour_item: checkpoint.age == 2, three_hour_item: checkpoint.age == 3, four_hour_item: checkpoint.age == 4}")
            include list_item
    div(ng-show="checkpoints.length == 0")
        span 目前沒回報
